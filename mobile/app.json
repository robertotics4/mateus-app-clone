{
  "expo": {
    "name": "Mateus App Clone",
    "slug": "mateusappclone",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "androidStatusBar": {
      "barStyle": "dark-content"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff0013"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
